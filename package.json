{
  "name": "react-web-h5",
  "version": "1.0.0",
  "description": "react mbox webpack",
  "main": "src/index.js",
  "author": "JUSTIN XU",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "setup": "yarn && yarn build-dll",
    "build-dll": "webpack --config build/webpack_dll.config.js --mode production",
    "dev": "webpack-dev-server --progress --colors --config build/webpack.dev.js",
    "start": "env-cmd start:dev yarn dev",
    "start-b": "BUNDLE=1 yarn dev",
    "release": "webpack --progress --colors --config build/webpack.prod.js --profile --json > stats.json",
    "build-s": "env-cmd build:staging yarn release",
    "build-p": "env-cmd build:prod yarn release"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
